{
	"manifest_version": 2,
	"name": "PhishMeNot",
	"version": "1.0",
	"description": "Phishing protection extension",
	"permissions": ["storage", "activeTab", "webRequest", "<all_urls>"],
	"background": {
	  "scripts": ["chrome.js"],
	  "persistent": false
	},
	"browser_action": {
	  "default_popup": "popup.html",
	  "default_icon": "images/icon.png"
	}
  }
  